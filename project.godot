[dotnet]

project/assembly_name="AmbienTest"

[editor_plugins]

enabled=PackedStringArray("res://addons/YouCanDoIt/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false

[global_group]

